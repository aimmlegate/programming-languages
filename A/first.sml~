(* year * month * day *)

fun is_older (dateTwo: int*int*int, dateOne: int*int*int) =
    let
	val diff = (#1 dateOne - #1 dateTwo, #2 dateOne - #2 dateTwo, #3 dateOne - #3 dateTwo )
    in
	if #1 diff > 0
	then
	    true
	else
	    if
		#2 diff > 0
	    then
		true
	    else
		if
		    #3 diff > 0
		then
		    true
		else
		    false
    end
	
fun number_in_month (dates: (int*int*int) list, month: int) =
    if
	null dates
    then
	0
    else
	
	let
	    fun in_m (date: int*int*int) =
		if
		    #2 date = month
		then
		    1
		else
		    0
	in
	    in_m(hd dates) + number_in_month(tl dates, month)
	end
	    
	    
